{
    "name": "kurl/silex-doctrine-migrations-provider",
    "description": "A doctrine migrations provider for silex",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.1",
        "doctrine/dbal": "^2.6",
        "pimple/pimple": "^3.0",
        "silex/api": "^2.0",
        "doctrine/migrations": "v2.0.0-alpha1",
        "symfony/console": "^3.4||^4.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "silex/silex": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": { "Kurl\\Silex\\Provider\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Kurl\\Silex\\Provider\\Tests\\": "tests/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
