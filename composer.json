{
    "name": "kurl/silex-doctrine-migrations-provider",
    "description": "A doctrine migrations provider for silex",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^5.5|^7"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "silex/silex": "~1.0",
        "symfony/console": "~2.3",
        "doctrine/dbal": "~2.2",
        "doctrine/migrations": "^1.0",
        "doctrine/orm": "~2.2"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "": "tests/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
